# Anthropic
ANTHROPIC_API_KEY=sk-ant-...

# Kalshi credentials
KALSHI_API_KEY_ID=your-api-key-id
KALSHI_PRIVATE_KEY_PATH=/workspace/keys/private_key.pem

# Trading environment: "demo" or "prod"
KALSHI_ENV=demo

# Trading limits
MAX_POSITION_USD=50.0
MAX_PORTFOLIO_USD=500.0
MAX_ORDER_COUNT=100
MIN_EDGE_PCT=5.0
KALSHI_FEE_RATE=0.03

# Agent SDK
AGENT_PROFILE=demo
AGENT_MODEL=claude-sonnet-4-5-20250929
AGENT_MAX_BUDGET_USD=1.0
